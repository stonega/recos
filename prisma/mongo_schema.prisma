generator client {
  provider = "prisma-client-js"
  output   = "./generated/mongo_client"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model subtitle {
  id_            String @id @default(auto()) @map("_id") @db.ObjectId
  endTimestamp   String @map("end_time")
  id             Int
  startTimestamp String @map("start_time")
  task_id        String
  text           String
  time           String
}
